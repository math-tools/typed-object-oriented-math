/***
----------------------------------------------------------
Injection from the integral domain into its quotient field
----------------------------------------------------------
***/

with (QF : QuotFieldOf (ID : IntDomain)) in {
// Initial integral domain can be embeded inside the quotient field.
  fix func cano_inj (elt : ID.InSet) -> InSet {
    return (
      QF.cano_surj (elt , ID.id_prod)
    )
  }
}
