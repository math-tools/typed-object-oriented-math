/***
============
Ordered sets
============

-------------
\Gene \struct
-------------
***/

type OrderedSet : Set as OS {
  func lt : InSet -> InSet -> \B

  axiom {
    anti_symmetry :=
      forall x in OS ,
      not (lt x x)

    transitivity :=
      forall x y z in OS ,
      (lt x y) and (lt y z) ==> (lt x z)
  }
}
