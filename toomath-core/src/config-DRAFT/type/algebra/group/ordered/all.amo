/***
==============
Ordered groups
==============
***/


for_toks_explicit_list kind [: "" | Compo | Abelian | Add | Prod :]
{
  type_ext_from "OrderedGroup{kind}"
                ("Group{kind}" + "OrderedMonoid")

  type_ext_from "TotalOrderedGroup{kind}"
                ("Group{kind}" + "TotalOrderedMonoid")
}
