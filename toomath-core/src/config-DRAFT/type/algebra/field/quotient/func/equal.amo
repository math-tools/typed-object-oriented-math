/***
note::
    To make ''equal'' computable, we use ''cano_surj''.
***/

with (QF : QuotFieldOf (ID : IntDomain)) in {
  func equal (x : QF.InSet)
          -> (y : QF.InSet)
          -> \B
  {
    return (
      match (x , y) with
        | (QF.cano_surj (x_num , x_deno) , QF.cano_surj (y_num , y_deno))
          ->
          ID.prod x_num y_deno = ID.prod y_num x_deno
    )
  }
}
